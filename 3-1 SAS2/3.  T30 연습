3. 위 수술 명세서 건수
data pr3_c;
set t30;
위수술=substr(MCARE_DIV_CD,1, 5);
run;
proc sql;
create table pr3 as
select a.*, b.구분 
from pr3_c as a
join cord as b on
a.위수술=b.코드;
quit;
proc sort data=pr3 out=pr3_e_m nodupkey;
by rn_key;
run;

