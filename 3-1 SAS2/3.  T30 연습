3. 위 수술 명세서 건수
option validvarname=any;
data t30;
set Tmp1.nsc2_m30;
run;

proc import
datafile='C:\Users\OWNER\Downloads\참고4_위암수술코드.xlsx'
out=cord dbms=xlsx replace;
sheet="Sheet1";
run;

data pr3_c;
set t30;
위수술=substr(MCARE_DIV_CD,1, 5);
run;
proc sql;
create table pr3 as
select a.*, b.구분 
from pr3_c as a
join cord as b on
a.위수술=b.코드;
quit;
proc sort data=pr3 out=pr3_e_m nodupkey;
by rn_key;
run;

/*
one step

option validvarname=any;
data t30;
set Tmp1.nsc2_m30;
run;

proc import
datafile='C:\Users\OWNER\Downloads\참고4_위암수술코드.xlsx'
out=cord dbms=xlsx replace;
sheet="Sheet1";
run;

proc sql;
create table pr3 as
select a.*, b.구분
from t30 as a
join cord as b on
substr(a.MCARE_DIV_CD,1,5)=substr(b.코드,1,5);
quit;
 proc sort data=pr3 out=pr3_e_m nodupkey;
by rn_key;
run;



*/


4. 연도별(2010-2015) 내시경/위절제술 명세서 건수
data pr4;
set pr3_e_m;
year=substr(MDCARE_STRT_DT,1,4);
if "2010"<=year<="2015";
run;
proc sort data=pr4 out=pr4_s nodupkey;
by RN_KEY 구분;
run;
proc freq data=pr4_s;
table year*구분/nopercent norow nocol;
run;

/
onestep

proc sql;
create table pr4 as
select a.*, substr(a.)

*/

